{
  "name": "extension-incubator",
  "version": "0.0.0",
  "description": "a playground .",
  "main": "index.js",
  "repository": "https://github.com/xyhp915/extension-incubator.git",
  "author": "charlie <xyhp915@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev:statics": "http-server --cors=\"*\" .",
    "dev:main": "parcel ./index.html",
    "dev:vue": "cd ext-vue && parcel --public-url=http://localhost:8080/ext-vue/dist/ ./index.html",
    "dev:react": "cd ext-react && parcel --public-url=http://localhost:8080/ext-react/dist/ ./index.html",
    "build:vue": "cd ext-vue && parcel build --public-url=/ext-vue --out-dir=../examples/ext-vue ./index.html",
    "build:react": "cd ext-react && parcel build --public-url=/ext-react --out-dir=../examples/ext-react ./index.html",
    "build:main": "parcel build --public-url=. --out-dir=./examples ./index.html",
    "build:ext-a": "parcel build --public-url=/ --out-dir=./examples ./ext-a.html",
    "build:ext-b": "parcel build --public-url=/ --out-dir=./examples ./ext-b.html"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "vue": "^2.6.12",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.2",
    "@vue/component-compiler-utils": "^3.2.0",
    "parcel-bundler": "^1.12.4",
    "typescript": "^4.0.3",
    "vue-template-compiler": "^2.6.12"
  },
  "browserslist": [
    "chrome 70"
  ]
}
